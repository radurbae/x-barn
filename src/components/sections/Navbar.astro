---
import type { NavLink } from "../../types/landing";

interface Props {
  links: NavLink[];
}

const { links } = Astro.props;
---

<header>
  <nav aria-label="Navigasi utama">
    <a href="#beranda">Kopi Senja</a>
    <ul>
      {links.map((link) => (
        <li>
          <a href={link.href}>{link.label}</a>
        </li>
      ))}
    </ul>
  </nav>
</header>
